{
	"name": "dataflow2",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DsADLSdynamicfolder",
						"type": "DatasetReference"
					},
					"name": "sourcefile"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "DSADLSdynamiconlyfolder",
						"type": "DatasetReference"
					},
					"name": "sink"
				}
			],
			"transformations": [
				{
					"name": "window1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          id as short,",
				"          name as string,",
				"          salary as integer,",
				"          gender as string,",
				"          deptno as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> sourcefile",
				"sourcefile window(asc(deptno, true),",
				"     RuntotalDeptno = sum(deptno)) ~> window1",
				"window1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          Salesman as string,",
				"          Q1 as string,",
				"          Q2 as string,",
				"          Q3 as string,",
				"          Q4 as string",
				"     ),",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> sink"
			]
		}
	}
}