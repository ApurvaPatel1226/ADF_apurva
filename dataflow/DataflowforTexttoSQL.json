{
	"name": "DataflowforTexttoSQL",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "dsadlscontainerxwithfile",
						"type": "DatasetReference"
					},
					"name": "sourcefromTextADLS"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "Dssqldataset",
						"type": "DatasetReference"
					},
					"name": "sinktodbtable"
				}
			],
			"transformations": [],
			"scriptLines": [
				"parameters{",
				"     SQLtableschema as string ('test'),",
				"     SQLtablename as string ('employeePay')",
				"}",
				"source(output(",
				"          {id     name      salary  gender deptnio} as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> sourcefromTextADLS",
				"sourcefromTextADLS sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          id as integer,",
				"          name as string,",
				"          salary as integer,",
				"          gender as string,",
				"          deptno as integer",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError') ~> sinktodbtable"
			]
		}
	}
}